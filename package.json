    "react-electron-web-view": "^2.0.1",
    "react-tabs": "next"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "electron-prebuilt-compile": "1.6.2",
    "react-hot-loader": "next"
  }
